{
    "builds": [
        {
            "src": "FE/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "FE/build"
            }
        },
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/FE/build/$1"
        }
    ]
}